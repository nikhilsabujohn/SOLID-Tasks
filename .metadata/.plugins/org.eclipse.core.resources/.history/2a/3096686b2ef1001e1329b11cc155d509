package openClosedPrinciple;

public class WhatsappNotification implements NotificationServices{

	@Override
	public void sendOTP() {
		int count=4;
		String otp=null;
		while(count!=0)
		{
		int n=(int) (Math.random()*10);
		if(n>=65 && n<=90)
		{
		char ch=(char)(n);
		otp+=ch;
		}
		}
		
	}
	

}
