package com.ty.listDupe;import java.util.zip.CRC32;

public class check {
	class node
	{
		int data;
		node next;
	
	public node(int data)
	{
		this.data=data;
		this.next=null;
	}
	}
	public node head;
	public node tail;
	
	
	
	public void addnode(int data)
	{
		node newnode= new node(data);
		if(head==null)
		{
			head=newnode;
			tail=newnode;
		}
		else
		{
			tail.next=newnode;
			tail=newnode;
		}
	}
	
	public void Display()
	{
		node current;
		current=head;
		if(head==null)
		{
			System.out.println("empty list");
		}
		else
		{
			while(current!=null)
			{
				System.out.println(current.data);
				current=current.next;
			}
		}
	}

}
